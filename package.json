{
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.49.0",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "qrcode-terminal": "^0.12.0",
    "supertest": "^7.1.0",
    "whatsapp-web.js": "^1.27.0"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "ts-patch install || true",
    "build": "tsc -p tsconfig.json",
    "start:prod": "node -r tsconfig-paths/register -r module-alias/register --require=./tsconfig-paths.config.js dist/app.js",
    "migrate": "npx dotenv-cli -e .env.local -- npx prisma migrate dev",
    "generate": "npx dotenv-cli -e .env.local -- npx prisma generate",
    "reset": "npx dotenv-cli -e .env.local -- npx prisma migrate reset",
    "dev": "nodemon --exec \"npx ts-node -r tsconfig-paths/register\" src/app.ts && src/lib/whatsapp/worker.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.1",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.6.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "ts-transform-paths": "^3.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
